{
  "manifest_version": 3,
  "name": "Last Tabs Tracker",
  "version": "1.0",
  "description": "Tracks tab recency for Raycast extension",
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "nativeMessaging"
  ],
  "host_permissions": [
    "http://127.0.0.1:8987/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Last Tabs Tracker"
  }
}
